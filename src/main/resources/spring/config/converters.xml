<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<bean class="converters.AdministratorToStringConverter"/>
		<bean class="converters.StringToAdministratorConverter"/>
		<bean class="converters.CreditCardToStringConverter"/>
		<bean class="converters.StringToCreditCardConverter"/>
		<bean class="converters.UrlToStringConverter"/>
		<bean class="converters.StringToUrlConverter"/>
		<bean class="converters.ConfigurationToStringConverter"/>
		<bean class="converters.StringToConfigurationConverter"/>
		<bean class="converters.ActorToStringConverter"/>
		<bean class="converters.StringToActorConverter"/>
		<bean class="converters.OfferToStringConverter"/>
		<bean class="converters.StringToOfferConverter"/>
		<bean class="converters.TransactionToStringConverter"/>
		<bean class="converters.StringToTransactionConverter"/>
		<bean class="converters.SponsorToStringConverter"/>
		<bean class="converters.StringToSponsorConverter"/>
		<bean class="converters.RefereeToStringConverter"/>
		<bean class="converters.StringToRefereeConverter"/>
		<bean class="converters.ReaderToStringConverter"/>
		<bean class="converters.StringToReaderConverter"/>
		<bean class="converters.OrganizerToStringConverter"/>
		<bean class="converters.StringToOrganizerConverter"/>
	</util:list>
	
</beans>